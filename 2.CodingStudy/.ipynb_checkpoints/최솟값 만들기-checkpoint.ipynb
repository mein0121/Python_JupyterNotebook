    "#최솟값 만들기\n",
    "def solution(A,B):\n",
    "    answer=0\n",
    "    A.sort()\n",
    "    B.sort(reverse=True)\n",
    "    for n1, n2 in zip(A,B):\n",
    "        answer += n1*n2\n",
    "\n",
    "    return answer"
   ]
